<odoo>
    <record id="view_instalacje24_warranty_tree" model="ir.ui.view">
        <field name="name">instalacje24.warranty.tree</field>
        <field name="model">instalacje24.warranty</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="partner_id"/>
                <field name="job_id"/>
                <field name="start_date"/>
                <field name="end_date"/>
                <field name="state"/>
            </tree>
        </field>
    </record>
    <record id="action_instalacje24_warranty" model="ir.actions.act_window">
        <field name="name">Gwarancje</field>
        <field name="res_model">instalacje24.warranty</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
