<odoo>
    <record id="view_instalacje24_complaint_tree" model="ir.ui.view">
        <field name="name">instalacje24.complaint.tree</field>
        <field name="model">instalacje24.complaint</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="partner_id"/>
                <field name="job_id"/>
                <field name="state"/>
            </tree>
        </field>
    </record>

    <record id="view_instalacje24_complaint_graph" model="ir.ui.view">
        <field name="name">instalacje24.complaint.graph</field>
        <field name="model">instalacje24.complaint</field>
        <field name="arch" type="xml">
            <graph string="Statystyka reklamacji" type="bar">
                <field name="state"/>
                <field name="id" type="measure"/>
            </graph>
        </field>
    </record>

    <record id="action_instalacje24_complaint" model="ir.actions.act_window">
        <field name="name">Reklamacje</field>
        <field name="res_model">instalacje24.complaint</field>
        <field name="view_mode">tree,form,graph</field>
    </record>
</odoo>
